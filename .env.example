# Supabase Configuration
# Get these values from your Supabase project settings

# Supabase project URL
SUPABASE_URL=your_project_url_here

# Supabase anonymous key (public key, safe for client-side)
SUPABASE_KEY=your_anon_key_here

# Supabase service role key (KEEP SECRET - server-only, bypasses RLS)
# Used for admin operations like username lookup
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# E2E Test Configuration
# E2E user credentials for Playwright tests
# This user must be created manually in your test database before running tests
E2E_USERNAME=testuser
E2E_PASSWORD=TestPassword123

# Cloudflare Configuration
# Required for deployment to Cloudflare Pages
# Get these from your Cloudflare dashboard
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
CLOUDFLARE_PROJECT_NAME=your_cloudflare_project_name_here
